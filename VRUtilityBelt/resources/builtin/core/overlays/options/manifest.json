{
    "name": "UB Options Overlay",
    "description": "Options Overlay for VR Utility Belt",
    "key": "vrub_core_options", /* Use something that won't collide with other overlays. */
    "entrypoint": "addon://overlays/options/index.html",
    "width": 1200,
    "height": 800,
    "meters": 2.5,
    "keyboard": true,
    "plugins": [
        "vrub_core_AddonManagement"
    ],
    "show_as_dashboard": true,
    "debug": false,
    "thumbnail": "icon.jpg",
    
    "render_models": [
        {
            "key": "hat",
            "model": "models/santa.obj",
            "width": 1,
            "height": 1,
            "meters": 1,
            "position": {
                "X": 1.3,
                "Y": 0.8,
                "Z": 0
            },
            "rotation": {
                "X": 0,
                "Y": 0,
                "Z": 35
            },
            "opacity": 1
        }
    ]
}
{
    "name": "UB Options Overlay",
    "description": "Options Overlay for VR Utility Belt",
    "key": "vrub_options", /* Use something that won't collide with other overlays. */
    "entrypoint": "addon://overlays/options/index.html",
    "flags": [
        "vr_keyboard", /* Inputs bring up SteamVR Keyboard */
        "stt", /* Speech to Text (maybe) */
        "toolbelt", /* Can be put on the 'toolbelt' (coming soon) */
    ],
    plugins: [
        "vrub_core_PersistentStore",
        "vrub_core_SuperSimplePlugin",
    ],
    "width": 1024,
    "height": 768,
    "type": "dashboard", /* Dashboard, Independent (Floating) */
    "debug": true,
}
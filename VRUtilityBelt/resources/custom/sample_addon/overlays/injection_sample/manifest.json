{
    "name": "Injection Example",
    "description": "Demonstrates JS and CSS Injection on the Google homepage",
    "key": "vrub_example_injection", /* Use something that won't collide with other overlays. */
    "entrypoint": "https://google.com",
    "inject": {
        "js": [
            "overlays/injection_sample/example.js",
        ],
        "css": [
            "overlays/injection_sample/example.css",
        ],
    },
    "plugins": [
        "vrub_core_keyboard",
    ],
    "width": 1024,
    "height": 768,
    "type": "dashboard", /* Dashboard, Independent (Floating) */
    "debug": false,
    "keyboard": true,
}